{
  "name": "@react-cad/core",
  "version": "1.0.0",
  "main": "lib/index.js",
  "author": "simlrh",
  "license": "LGPL",
  "scripts": {
    "setup": "docker build -t react-cad-build .",
    "build:wasm": "docker run --rm -i -u=`id -u` -v $PWD:/src/react-cad react-cad-build",
    "build:ts": "tsc",
    "build": "run-s build:wasm build:ts",
    "build:wasm:watch": "docker run --rm -i -e 'BUILD_WATCH=true' -u=`id -u` -v $PWD:/src/react-cad react-cad-build",
    "build:ts:watch": "tsc --watch",
    "start": "run-p build:wasm:watch build:ts:watch"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
